{
    "name": "stagehand-mcp",
    "version": "1.0.8",
    "description": "MCP server that exposes Stagehand browser automation capabilities",
    "main": "dist/index.js",
    "type": "module",
    "bin": {
        "stagehand-mcp": "dist/index.js"
    },
    "scripts": {
        "build": "tsc && chmod +x dist/index.js",
        "start": "node dist/index.js",
        "dev": "tsc --watch",
        "inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@browserbasehq/stagehand": "^2.0.0",
        "@modelcontextprotocol/sdk": "^1.17.2",
        "express": "^4.21.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/node": "^22.0.0",
        "typescript": "^5.6.0"
    },
    "keywords": [
        "mcp",
        "stagehand",
        "browser-automation",
        "ai",
        "model-context-protocol"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robrichardson13/stagehand-mcp.git"
    },
    "bugs": {
        "url": "https://github.com/robrichardson13/stagehand-mcp/issues"
    },
    "homepage": "https://github.com/robrichardson13/stagehand-mcp#readme",
    "author": "robrichardson13",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    }
}
